<mujoco model="ur5e scene">
  <include file="ur5e_gripper.xml"/>

  <statistic center="0.3 0 0.3" extent="0.8" meansize="0.08"/>

  <visual>
    <headlight diffuse="0.6 0.6 0.6" ambient="0.1 0.1 0.1" specular="0 0 0"/>
    <global azimuth="120" elevation="-20"/>
  </visual>

  <asset>
    <texture name="grid" type="2d" builtin="checker" rgb1=".2 .3 .4" rgb2=".1 0.15 0.2" width="512" height="512"
      mark="cross" markrgb=".8 .8 .8"/>
    <material name="grid" texture="grid" texrepeat="1 1" texuniform="true"/>
  </asset>

  <worldbody>
    <light pos="0 0 1.5" directional="true"/>
    <geom name="floor" size="1 1 0.01" type="plane" material="grid"/>
    <body name="target" pos="0.5 0 .5" quat="0 1 0 0" mocap="true">
      <geom type="box" size=".05 .05 .05" contype="0" conaffinity="0" rgba=".6 .3 .3 .2"/>
    </body>
    <body name="wall" pos="0.5 0 0.1">
      <geom name="wall" type="box" size=".1 .1 .1"/>
    </body>
  </worldbody>

  <sensor>
    <fromto geom1="shoulder_col"  geom2="wall"  cutoff="1"/>
    <fromto geom1="upper_arm_col0" geom2="wall" cutoff="1"/>
    <fromto geom1="upper_arm_col1" geom2="wall" cutoff="1"/>
    <fromto geom1="forearm_col0"   geom2="wall" cutoff="1"/>
    <fromto geom1="forearm_col1"   geom2="wall" cutoff="1"/>
    <fromto geom1="wrist_1_col"    geom2="wall" cutoff="1"/>
    <fromto geom1="wrist_2_col"    geom2="wall" cutoff="1"/>
    <fromto geom1="wrist_3_col"    geom2="wall" cutoff="1"/>
    
