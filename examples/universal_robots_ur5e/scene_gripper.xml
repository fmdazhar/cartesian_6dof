    <fromto geom1="base_gripper_col" geom2="wall" cutoff="1"/>
    <fromto geom1="right_driver_col" geom2="wall" cutoff="1"/>
    <fromto geom1="right_coupler_col" geom2="wall" cutoff="1"/>
    <fromto geom1="right_spring_col" geom2="wall" cutoff="1"/>
    <fromto geom1="right_follower_col"   geom2="wall" cutoff="1"/>

